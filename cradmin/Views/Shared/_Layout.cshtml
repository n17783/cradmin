                    </li>
                    <li class="treeview">
                        <a href="#">
                            <i class="fa fa-files-o"></i>
                            <span>Validation Process</span>
                            <span class="pull-right-container">
                                <span class="label label-primary pull-right">4</span>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="#" data-ng-click="LoadUserControls('CandidatesForValidation')"><i class="fa fa-circle-o"></i>Candidates For Validation</a></li>
                            <li><a href="#" data-ng-click="LoadUserControls('ValidatedCandidates')"><i class="fa fa-circle-o"></i>Validated Candidates</a></li>
                        </ul>
                    </li>

</ul>
            </section>
            <!-- /.sidebar -->
        </aside>
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper" id="contentLoader">
            <!-- Content Header (Page header) -->
            @RenderBody()
            <!-- /.content -->
        </div>
        <div class="mainLoader" id="rclLoader" style="display: none;">
            <div class="mainLoaderImg"></div>
            <div class="lyfBg"></div>
        </div>
        <!-- /.content-wrapper -->
        <footer class="main-footer">
            <div class="pull-right hidden-xs">
                <b>Version</b> 2.4.0
