                            <li><a href="#" data-ng-click="LoadUserControls('Trade')"><i class="fa fa-circle-o"></i>Trade </a></li>
                            <li><a href="#" data-ng-click="LoadUserControls('Role')"><i class="fa fa-circle-o"></i>Role</a></li>
                            <li><a href="#" data-ng-click="LoadUserControls('RollFormMapping')"><i class="fa fa-circle-o"></i>Roll Form Mapping</a></li>
                            <li><a href="#" data-ng-click="LoadUserControls('CourseMaster')"><i class="fa fa-circle-o"></i>Course Master</a></li>
                            <li><a href="#" data-ng-click="LoadUserControls('ValidationAgency')"><i class="fa fa-circle-o"></i>Validation Agency</a></li>
                            <li><a href="#" data-ng-click="LoadUserControls('ValidationTest')"><i class="fa fa-circle-o"></i>Validation Test</a></li>
                            <li><a href="#" data-ng-click="LoadUserControls('ValidationTestQuetion')"><i class="fa fa-circle-o"></i>Validation Test Quetion</a></li>
