<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>CR ASIA | Dashboard</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    @Styles.Render("~/Content/AdminLayout/css")
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">
    @Scripts.Render("~/Scripts/AdminLayout/jquery-2.2.3.min.js")

    <!-- jQuery UI 1.11.4 -->
    @Scripts.Render("~/Scripts/AdminLayout/jquery-ui.min.js")
    <!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
    <script>
  $.widget.bridge('uibutton', $.ui.button);
    </script>
    <!-- Bootstrap 3.3.7 -->
    @Scripts.Render("~/Scripts/AdminLayout/bootstrap.min.js")
    <!-- Morris.js charts -->
    @Scripts.Render("~/Scripts/AdminLayout/raphael.min.js")
    @Scripts.Render("~/Scripts/AdminLayout/morris.min.js")
    <!-- Sparkline -->
    @Scripts.Render("~/Scripts/AdminLayout/jquery.sparkline.min.js")
    <!-- jvectormap -->
    @Scripts.Render("~/Scripts/AdminLayout/jquery-jvectormap-1.2.2.min.js")
    @Scripts.Render("~/Scripts/AdminLayout/jquery-jvectormap-world-mill-en.js")
    <!-- jQuery Knob Chart -->
    <!-- daterangepicker -->
    @Scripts.Render("~/Scripts/AdminLayout/moment.min.js")
    @Scripts.Render("~/Scripts/AdminLayout/daterangepicker.js")
    <!-- datepicker -->
    @Scripts.Render("~/Scripts/AdminLayout/bootstrap-datepicker.min.js")
    <!-- Bootstrap WYSIHTML5 -->
    @Scripts.Render("~/Scripts/AdminLayout/bootstrap3-wysihtml5.all.min.js")
    <!-- Slimscroll -->
    @Scripts.Render("~/Scripts/AdminLayout/jquery.slimscroll.min.js")
    <!-- FastClick -->
    @Scripts.Render("~/Scripts/AdminLayout/fastclick.js")
    <!-- AdminLTE App -->
    @Scripts.Render("~/Scripts/AdminLayout/adminlte.min.js")
    <!-- AdminLTE dashboard demo (This is only for demo purposes) -->
    <!-- AdminLTE for demo purposes -->
    @Scripts.Render("~/Scripts/AdminLayout/jquery.knob.min.js")
    @Scripts.Render("~/Scripts/AdminLayout/datehelper.js")
    @Scripts.Render("~/Scripts/AdminLayout/angular.min.js")
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.6.9/angular-route.js"></script>
    @Scripts.Render("~/Scripts/AdminLayout/App.js")
    @Scripts.Render("~/Scripts/Adminlayout/common.js")
    @Scripts.Render("~/Scripts/Adminlayout/ShowCustomAlert.js")

    
    <style>
               .submitPopup {
	display: none;
	z-index: 70000;
	position: absolute;
}
.overlay {
	background: #fff;
	bottom: 0;
	opacity: 0;
	position: fixed;
	top: 0;
	width: 100%;
	z-index: 1001;
}



#alertBox .overlay {
	background: #000;
	opacity: 0.8;
}
.submitPopupPanel {
	background: #e9edf0 !important;
	color: #000 !important;
	font-size: 1.1em;
	/*margin-top:-95px;*/
	margin-left: -150px;
	position: fixed;
	text-align: center;
	top: 20%;
	left: 50%;
	width: 300px;
	z-index: 70000;
	padding: 15px;
	border-radius: 8px;
	box-shadow: 0px 5px 15px rgba(0,0,0,0.15);
	border: 3px solid #dfe4e8;
}
.submitPopupPanel .legendBtn {
	margin: 20px 0 0 0;
	width: 100%;
}
.submitPopupPanel .legendBtn li:last-child {
	margin: 0;
}
.submitPopupPanel .legendBtn li {
	list-style-type: none;
	display: inline-block;
	margin: 0 10px 0 0;
}

    </style>
</head>
<body class="hold-transition skin-blue sidebar-mini" data-ng-app="CRAdminApp" data-ng-controller="TemplateController">
    <div class="wrapper">
        <header class="main-header">
            <a href="#" class="logo">
                <img src="@Url.Content("~/Content/Images/logo.png")" class="logo" />
            </a>
            <nav class="navbar navbar-static-top">
                <a href="#" class="sidebar-toggle" data-toggle="push-menu" role="button">
                    <span class="sr-only">Toggle navigation</span>
                </a>
                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">
                        <!-- User Account: style can be found in dropdown.less -->
                        <li class="dropdown user user-menu">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <img src="~/Content/Images/user2-160x160.jpg" class="user-image" alt="User Image">
                                <span class="hidden-xs">Naresh Shinde</span>
                            </a>
                            <ul class="dropdown-menu">
                                <!-- User image -->
                                <li class="user-header">
                                    <img src="~/Content/Images/user2-160x160.jpg" class="img-circle" alt="User Image">
                                    <p>
                                        Naresh Shinde - Web Developer
                                        <small>Member since Nov. 2012</small>
                                    </p>
                                </li>
                                <!-- Menu Footer-->
                                <li class="user-footer">
                                    <div class="pull-left">
                                        <a href="#" class="btn btn-default btn-flat">Profile</a>
                                    </div>
                                    <div class="pull-right">
                                        <a href="#" class="btn btn-default btn-flat" onclick="LogOff()">Sign out</a>
                                    </div>
                                </li>
                            </ul>
                        </li>
                        
                    </ul>
                </div>
            </nav>
